<plexus>

  <lifecycle-handler-manager implementation="org.codehaus.plexus.lifecycle.DefaultLifecycleHandlerManager">
    <default-lifecycle-handler-id>plexus-configurable</default-lifecycle-handler-id>
  </lifecycle-handler-manager>
  
  <components>
        
    <component>
      <role>org.codehaus.xfire.XFire</role>
      <implementation>org.codehaus.xfire.plexus.PlexusXFire</implementation>
      <lifecycle-handler>plexus-configurable</lifecycle-handler>
      <configuration/>
    </component>
        
    <component>
      <role>org.codehaus.xfire.service.ServiceRegistry</role>
      <implementation>org.codehaus.xfire.plexus.ServiceRegistry</implementation>
      <lifecycle-handler>plexus-configurable</lifecycle-handler>
      <configuration/>
    </component>
    
    <component>
      <role>org.codehaus.xfire.transport.TransportManager</role>
      <implementation>org.codehaus.xfire.plexus.transport.PlexusTransportManager</implementation>
      <lifecycle-handler>plexus-configurable</lifecycle-handler>
      <configuration/>
      <requirements>
        <requirement>
          <role>org.codehaus.xfire.service.ServiceRegistry</role>
        </requirement>
      </requirements>
    </component>
                    
    <component>
      <role>org.codehaus.xfire.fault.FaultHandler</role>
      <role-hint>1.2</role-hint>
      <implementation>org.codehaus.xfire.fault.SOAP12FaultHandler</implementation>
      <lifecycle-handler>plexus-configurable</lifecycle-handler>
      <configuration/>
    </component>
    
    <component>
      <role>org.codehaus.xfire.fault.FaultHandler</role>
      <role-hint>1.1</role-hint>
      <implementation>org.codehaus.xfire.fault.SOAP11FaultHandler</implementation>
      <lifecycle-handler>plexus-configurable</lifecycle-handler>
      <configuration/>
    </component>
        
    <component>
      <role>org.codehaus.xfire.plexus.config.ConfigurationService</role>
      <implementation>org.codehaus.xfire.plexus.config.DefaultConfigurationService</implementation>
      <lifecycle-handler>plexus-configurable</lifecycle-handler>
      <configuration/>
      <requirements>
        <requirement>
          <role>org.codehaus.xfire.plexus.config.Configurator</role>
          <role-hint>org.codehaus.xfire.service.object.ObjectService</role-hint>
        </requirement>
      </requirements>
    </component>
            
    <component>
      <role>org.codehaus.xfire.plexus.config.Configurator</role>
      <role-hint>org.codehaus.xfire.service.object.ObjectService</role-hint>
      <implementation>org.codehaus.xfire.plexus.config.ObjectServiceConfigurator</implementation>
      <lifecycle-handler>plexus-configurable</lifecycle-handler>
      <configuration/>
    </component>

    <component>
      <role>org.codehaus.xfire.type.TypeMappingRegistry</role>
      <implementation>org.codehaus.xfire.plexus.type.TypeMappingRegistry</implementation>
      <lifecycle-handler>plexus-configurable</lifecycle-handler>
      <configuration>
        
        <!-- SOAP 1.1 encoding. -->
        <typeMapping namespace="http://schemas.xmlsoap.org/soap/encoding/">
        
          <!-- XMLSchema types -->
          <type class="java.lang.String"
                namespace="http://www.w3.org/2001/XMLSchema"
                name="string"
                type="org.codehaus.xfire.type.basic.StringType" />
        
          <type class="java.lang.Integer"
                namespace="http://www.w3.org/2001/XMLSchema"
                name="int"
                type="org.codehaus.xfire.type.basic.IntType" />
        
          <type class="java.lang.Float"
                namespace="http://www.w3.org/2001/XMLSchema"
                name="float"
                type="org.codehaus.xfire.type.basic.FloatType" />
        
          <type class="java.lang.Double"
                namespace="http://www.w3.org/2001/XMLSchema"
                name="double"
                type="org.codehaus.xfire.type.basic.DoubleType" />
        
          <type class="java.lang.Long"
                namespace="http://www.w3.org/2001/XMLSchema"
                name="long"
                type="org.codehaus.xfire.type.basic.LongType" />
                
          <type class="java.lang.Boolean"
                namespace="http://www.w3.org/2001/XMLSchema"
                name="boolean"
                type="org.codehaus.xfire.type.basic.BooleanType" />
        
          <type class="java.util.Calendar"
                namespace="http://www.w3.org/2001/XMLSchema"
                name="dateTime"
                type="org.codehaus.xfire.type.basic.CalendarType" />
        
          <type class="java.util.Date"
                namespace="http://www.w3.org/2001/XMLSchema"
                name="dateTime"
                type="org.codehaus.xfire.type.basic.DateType" /> 
 
          <!-- SOAP Encoded types -->
          <!-- type class="java.lang.String"
                namespace="http://schemas.xmlsoap.org/soap/encoding/"
                name="string"
                type="org.codehaus.xfire.type.basic.StringType" />
        
          <type class="java.lang.Integer"
                namespace="http://schemas.xmlsoap.org/soap/encoding/"
                name="int"
                type="org.codehaus.xfire.type.basic.IntType" />
        
          <type class="java.util.Calendar"
                namespace="http://schemas.xmlsoap.org/soap/encoding/"
                name="dateTime"
                type="org.codehaus.xfire.type.basic.CalendarType" />
        
          <type class="java.util.Date"
                namespace="http://schemas.xmlsoap.org/soap/encoding/"
                name="date"
                type="org.codehaus.xfire.type.basic.DateType" / -->
               
        </typeMapping>
		
		<!-- SOAP 1.2 encoding. -->
		<typeMapping namespace="http://www.w3.org/2003/05/soap-encoding/"
                 default="true">
        
          <!-- XMLSchema types -->
          <type class="java.lang.String"
                namespace="http://www.w3.org/2001/XMLSchema"
                name="string"
                type="org.codehaus.xfire.type.basic.StringType" />
                        
          <type class="java.lang.Boolean"
                namespace="http://www.w3.org/2001/XMLSchema"
                name="boolean"
                type="org.codehaus.xfire.type.basic.BooleanType" />
                
          <type class="java.lang.Integer"
                namespace="http://www.w3.org/2001/XMLSchema"
                name="int"
                type="org.codehaus.xfire.type.basic.IntType" />
                
          <type class="java.lang.Float"
                namespace="http://www.w3.org/2001/XMLSchema"
                name="float"
                type="org.codehaus.xfire.type.basic.FloatType" />
        
          <type class="java.lang.Double"
                namespace="http://www.w3.org/2001/XMLSchema"
                name="double"
                type="org.codehaus.xfire.type.basic.DoubleType" />
        
          <type class="java.lang.Long"
                namespace="http://www.w3.org/2001/XMLSchema"
                name="long"
                type="org.codehaus.xfire.type.basic.LongType" />
                
          <type class="java.util.Calendar"
                namespace="http://www.w3.org/2001/XMLSchema"
                name="dateTime"
                type="org.codehaus.xfire.type.basic.CalendarType" />
        
          <type class="java.util.Date"
                namespace="http://www.w3.org/2001/XMLSchema"
                name="dateTime"
                type="org.codehaus.xfire.type.basic.DateType" />
 
          <!-- SOAP Encoded types -->
          <!-- type class="java.lang.String"
                namespace="http://schemas.xmlsoap.org/soap/encoding/"
                name="string"
                type="org.codehaus.xfire.type.basic.StringType" />
        
          <type class="java.lang.Integer"
                namespace="http://schemas.xmlsoap.org/soap/encoding/"
                name="int"
                type="org.codehaus.xfire.type.basic.IntType" />
        
          <type class="java.util.Calendar"
                namespace="http://schemas.xmlsoap.org/soap/encoding/"
                name="dateTime"
                type="org.codehaus.xfire.type.basic.CalendarType" />
        
          <type class="java.util.Date"
                namespace="http://schemas.xmlsoap.org/soap/encoding/"
                name="date"
                type="org.codehaus.xfire.type.basic.DateType" / -->
               
        </typeMapping>

        <!-- XSD/Literal types -->
        <typeMapping namespace="http://www.w3.org/2001/XMLSchema">
        
          <type class="java.lang.String"
                namespace="http://www.w3.org/2001/XMLSchema"
                name="string"
                type="org.codehaus.xfire.type.basic.StringType" />
        
          <type class="java.lang.Boolean"
                namespace="http://www.w3.org/2001/XMLSchema"
                name="string"
                type="org.codehaus.xfire.type.basic.BooleanType" />
                
          <type class="java.lang.Integer"
                namespace="http://www.w3.org/2001/XMLSchema"
                name="int"
                type="org.codehaus.xfire.type.basic.IntType" />
                
          <type class="java.lang.Float"
                namespace="http://www.w3.org/2001/XMLSchema"
                name="float"
                type="org.codehaus.xfire.type.basic.FloatType" />
        
          <type class="java.lang.Double"
                namespace="http://www.w3.org/2001/XMLSchema"
                name="double"
                type="org.codehaus.xfire.type.basic.DoubleType" />
        
          <type class="java.lang.Long"
                namespace="http://www.w3.org/2001/XMLSchema"
                name="long"
                type="org.codehaus.xfire.type.basic.LongType" />
                
          <type class="java.util.Calendar"
                namespace="http://www.w3.org/2001/XMLSchema"
                name="dateTime"
                type="org.codehaus.xfire.type.basic.CalendarType" />
        
          <type class="java.util.Date"
                namespace="http://www.w3.org/2001/XMLSchema"
                name="dateTime"
                type="org.codehaus.xfire.type.basic.DateType" />
                
        </typeMapping>
        
        
      </configuration>
    </component>
  </components>
  
</plexus>
