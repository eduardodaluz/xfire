<?xml version="1.0" encoding="UTF-8"?>
<project>
    <extend>${basedir}/../project.xml</extend>
    <artifactId>xfire-annotations</artifactId>
    <name>Annotations XFire Module</name>
    <package>org.codehaus.xfire.annotations</package>
    <shortDescription>Annotations support for XFire.</shortDescription>
    <description>Annotations (JSR-175) support for XFire.</description>
    <siteDirectory>/home/projects/xfire/public_html/maven/xfire/multiproject/${pom.artifactId}</siteDirectory>
    <dependencies>
        <dependency>
            <groupId>xfire</groupId>
            <artifactId>xfire-core</artifactId>
            <version>${pom.currentVersion}</version>
            <properties>
                <eclipse.dependency>true</eclipse.dependency>
            </properties>
            <url>${pom.url}</url>
        </dependency>
        <dependency>
            <groupId>xfire</groupId>
            <artifactId>xfire-java</artifactId>
            <version>${pom.currentVersion}</version>
            <properties>
                <eclipse.dependency>true</eclipse.dependency>
            </properties>
            <url>${pom.url}</url>
        </dependency>
        <dependency>
            <groupId>commons-beanutils</groupId>
            <artifactId>commons-beanutils</artifactId>
            <version>1.7.0</version>
            <url>http://jakarta.apache.org/commons/beanutils/</url>
        </dependency>
        <dependency>
            <groupId>commons-attributes</groupId>
            <artifactId>commons-attributes-api</artifactId>
            <version>2.1</version>
            <url>http://jakarta.apache.org/commons/attributes/</url>
        </dependency>
        <dependency>
            <groupId>backport175</groupId>
            <artifactId>backport175</artifactId>
            <version>1.0.RC1</version>
            <url>http://backport175.codehaus.org</url>
        </dependency>
        <!--
        
        Test only dependencies
        
        -->
        <dependency>
            <groupId>easymock</groupId>
            <artifactId>easymock</artifactId>
            <version>1.1</version>
            <url>http://www.easymock.org/</url>
        </dependency>
        <!--
        
        Maven Plugins
        
        -->
        <dependency>
            <groupId>commons-attributes</groupId>
            <artifactId>commons-attributes-plugin</artifactId>
            <version>2.1</version>
            <type>plugin</type>
            <url>http://jakarta.apache.org/commons/attributes/</url>
        </dependency>
    </dependencies>
</project>
