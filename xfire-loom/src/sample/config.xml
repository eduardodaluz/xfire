                  type="org.codehaus.xfire.java.type.CalendarType" />

            <type class="java.util.Date"
                  namespace="http://schemas.xmlsoap.org/soap/encoding/"
                  name="date"
                  type="org.codehaus.xfire.java.type.DateType" / -->

        </typeMapping>

        <!-- SOAP 1.2 encoding. -->
        <typeMapping namespace="http://www.w3.org/2003/05/soap-encoding/" default="true">

            <!-- XMLSchema types -->
            <type class="java.lang.String"
                namespace="http://www.w3.org/2001/XMLSchema"
                name="string"
                type="org.codehaus.xfire.java.type.StringType"/>

            <type class="java.lang.Boolean"
                namespace="http://www.w3.org/2001/XMLSchema"
                name="boolean"
                type="org.codehaus.xfire.java.type.BooleanType"/>

            <type class="java.lang.Integer"
                namespace="http://www.w3.org/2001/XMLSchema"
                name="int"
                type="org.codehaus.xfire.java.type.IntType"/>

            <type class="java.lang.Float"
                namespace="http://www.w3.org/2001/XMLSchema"
                name="float"
                type="org.codehaus.xfire.java.type.FloatType"/>

            <type class="java.lang.Double"
                namespace="http://www.w3.org/2001/XMLSchema"
                name="double"
                type="org.codehaus.xfire.java.type.DoubleType"/>

            <type class="java.lang.Long"
                namespace="http://www.w3.org/2001/XMLSchema"
                name="long"
                type="org.codehaus.xfire.java.type.LongType"/>

            <type class="java.util.Calendar"
                namespace="http://www.w3.org/2001/XMLSchema"
                name="dateTime"
                type="org.codehaus.xfire.java.type.CalendarType"/>

            <type class="java.util.Date"
                namespace="http://www.w3.org/2001/XMLSchema"
                name="dateTime"
                type="org.codehaus.xfire.java.type.DateType"/>

            <type class="java.math.BigDecimal"
                  namespace="http://www.w3.org/2001/XMLSchema"
                  name="decimal"
                  type="org.codehaus.xfire.java.type.BigDecimalType" />

            <!-- SOAP Encoded types -->
            <!-- type class="java.lang.String"
            namespace="http://schemas.xmlsoap.org/soap/encoding/"
            name="string"
            type="org.codehaus.xfire.java.type.StringType" />

            <type class="java.lang.Integer"
            namespace="http://schemas.xmlsoap.org/soap/encoding/"
