<project>
  <pomVersion>1</pomVersion>
  <name>XMLBeans XFire Module</name>
  <groupId>xfire</groupId>
  <artifactId>xfire-xmlbeans</artifactId>
  <id>xfire-xmlbeans</id>
  <extend>${maven.repo.local}/xfire/poms/xfire-1.0-M3.pom</extend>

  <package>org.codehaus.xfire</package>
  <description>An XMLBeans binding module for XFire.</description>
  <shortDescription>An XMLBeans binding module for XFire.</shortDescription>
  
  <siteDirectory>/home/projects/xfire/public_html/maven/xfire-xmlbeans</siteDirectory>
  
  <versions/>
  
  <branches/>
  
  <dependencies>

    <dependency>
      <groupId>xfire</groupId>
      <artifactId>xfire</artifactId>
      <version>1.0-M3</version>
    </dependency>

    <dependency>
      <groupId>xfire</groupId>
      <artifactId>xfire-java</artifactId>
      <version>1.0-M3</version>
    </dependency>
	
    <dependency>
      <groupId>xfire</groupId>
      <artifactId>xmlbeans</artifactId>
      <version>2.0-20041214</version>
    </dependency>
    
    <dependency>
      <groupId>xfire</groupId>
      <artifactId>xmlbeans-xmlpublic</artifactId>
      <version>2.0-20041214</version>
    </dependency>

    <!-- Generator Dependencies -->
    <dependency>
      <groupId>ant</groupId>
      <artifactId>ant</artifactId>
      <version>1.6.2</version>
    </dependency>

    <dependency>
      <groupId>log4j</groupId>
      <artifactId>log4j</artifactId>
      <version>1.2.8</version>
    </dependency>
        
    <dependency>
      <groupId>commons-collections</groupId>
      <artifactId>commons-collections</artifactId>
      <version>2.1</version>
    </dependency>
            
    <dependency>
      <groupId>velocity</groupId>
      <artifactId>velocity</artifactId>
      <version>1.4</version>
    </dependency>
    
  </dependencies>
  
  <build>
    <sourceDirectory>${basedir}/src/main</sourceDirectory>
    <unitTestSourceDirectory>${basedir}/src/test</unitTestSourceDirectory>
    <unitTest>
      <includes>
        <include>**/*Test.java</include>
        <include>**/*TestCase.java</include>
      </includes>
      <excludes>
        <exclude>**/*Abstract*.java</exclude>
      </excludes>
      <resources>
        <resource>
          <directory>${basedir}/src/test</directory>
          <includes>
            <include>**/*.xml</include>
            <include>**/*.xsb</include>
            <include>**/*.vm</include>
          </includes>
          <filtering>false</filtering>
        </resource>
      </resources>
    </unitTest>
    
    <resources>
      <resource>
        <directory>${basedir}/src/main</directory>
        <includes>
          <include>**/*.xml</include>
          <include>**/*.xsb</include>
          <include>**/*.vm</include>
        </includes>
        <filtering>false</filtering>
      </resource>
    </resources>
    
  </build>

  <properties/>
</project>